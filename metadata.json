{
  "name": "puppet-misp",
  "version": "0.4.1-rc0",
  "author": "Vox Pupuli",
  "summary": "Module to install MISP (Malware Information Sharing Platform)",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-misp",
